{
  "name": "rwkextractor",
  "version": "1.0.0",
  "description": "Extract level files from RWK web version",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts-node": "ts-node",
    "build": "tsc",
    "bundle:win": "npm install && npm run build && npm prune --production && zip -r win.zip -xi node_modules -xi build -xi .env -xi run.cmd",
    "dev": "ts-node src/app.ts",
    "start": "node --unhandled-rejections=strict --inspect=5859 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrMasterplan/rwklevelfiles.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/MrMasterplan/rwklevelfiles/issues"
  },
  "homepage": "https://github.com/MrMasterplan/rwklevelfiles#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.37",
    "@types/puppeteer": "^5.4.3",
    "@types/rimraf": "^3.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "idb": "^6.0.0",
    "inquirer": "^8.0.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2"
  }
}
