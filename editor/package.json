{
  "name": "rwkextractor",
  "version": "2.1.0",
  "description": "Extract level files from RWK web version",
  "main": "app.ts",
  "scripts": {
    "pack": "npx webpack",
    "setup": "node --unhandled-rejections=strict -r ts-node/register ./tiled/setup.ts",
    "install": "npm run pack && npm run setup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts-node": "ts-node",
    "build": "tsc",
    "dev": "ts-node src/app.ts",
    "start": "node --unhandled-rejections=strict --inspect=5859 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrMasterplan/rwklevelfiles.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/MrMasterplan/rwklevelfiles/issues"
  },
  "homepage": "https://github.com/MrMasterplan/rwklevelfiles#readme",
  "devDependencies": {
    "@types/bitwise": "^2.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.37",
    "@types/pngjs": "^6.0.0",
    "@types/puppeteer": "^5.4.3",
    "@types/rimraf": "^3.0.0",
    "@types/xml-js": "^1.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bitwise": "^2.1.0",
    "canvas": "^2.7.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "idb": "^6.0.0",
    "inquirer": "^8.0.0",
    "pngjs": "^6.0.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.1.1",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0"
  }
}
