{
  "name": "TiledExtensionForKitty",
  "version": "1.0.0",
  "description": "Adds .kitty file support to the Tiled editor",
  "main": "app.ts",
  "scripts": {
    "build": "tsc && browserify -g [ babelify --presets [ @babel/preset-env ] ]  --entry build/app.js > build/kitty_bundle.js",
    "install": "npm run build && npm run setup",
    "dev": "ts-node src/app.ts",
    "start": "node --unhandled-rejections=strict --inspect=5859 -r ts-node/register ./src/app.ts",
    "setup": "node --unhandled-rejections=strict -r ts-node/register ./src/setup.ts",
    "start:watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrMasterplan/rwklevelfiles.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/MrMasterplan/rwklevelfiles/issues"
  },
  "homepage": "https://github.com/MrMasterplan/rwklevelfiles#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.10.3",

    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "parcel-bundler": "",
    "@types/node": "^14.14.37",
    "tiled-types": "^1.3.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
